{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "SaikaADFInstance"
		},
		"SaiDataLake_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'SaiDataLake'"
		},
		"ssk2616_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ssk2616'"
		},
		"LS_ADLS_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'LS_ADLS'"
		},
		"LS_Sql_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LS_Sql'"
		},
		"SaiDataLake_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://sskdatalake.dfs.core.windows.net/"
		},
		"ADLSEventTrigger_properties_typeProperties_scope": {
			"type": "string",
			"defaultValue": "/subscriptions/0fc4d8e8-8f21-4bd6-a59d-692b2a87791e/resourceGroups/SaiResourceGroup/providers/Microsoft.Storage/storageAccounts/sskdatalake"
		},
		"DailyMidDayTrigger_properties_DynamicCopy_parameters_fileName": {
			"type": "string",
			"defaultValue": "data"
		},
		"LS_ADLS_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://sskdatalake.dfs.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "SaikaADFInstance_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"SaiDataLake_accountKey": {
						"value": "[parameters('SaiDataLake_accountKey')]"
					},
					"ssk2616_connectionString": {
						"value": "[parameters('ssk2616_connectionString')]"
					},
					"LS_ADLS_accountKey": {
						"value": "[parameters('LS_ADLS_accountKey')]"
					},
					"LS_Sql_connectionString": {
						"value": "[parameters('LS_Sql_connectionString')]"
					},
					"SaiDataLake_properties_typeProperties_url": {
						"value": "[parameters('SaiDataLake_properties_typeProperties_url')]"
					},
					"ADLSEventTrigger_properties_typeProperties_scope": {
						"value": "[parameters('ADLSEventTrigger_properties_typeProperties_scope')]"
					},
					"LS_ADLS_properties_typeProperties_url": {
						"value": "[parameters('LS_ADLS_properties_typeProperties_url')]"
					},
					"DailyMidDayTrigger_properties_DynamicCopy_parameters_fileName": {
						"value": "[parameters('DailyMidDayTrigger_properties_DynamicCopy_parameters_fileName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}